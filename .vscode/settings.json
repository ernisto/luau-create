{
  "editor.formatOnSave": true,
  "files.associations": {},
  "files.eol": "\n",
  "files.exclude": {
    "*-lock.*": true,
    "*.lock": true,
    "*.lock.*": true,
    "*_packages/**": true,
    ".env": true,
    ".github": true,
    ".luaurc": true,
    ".pesde": true,
    ".vscode": true,
    "pesde.toml": true,
    "stylua.toml": true
  },
  "files.readonlyInclude": {
